<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
//테스트해볼게요
    <TextView
        android:id="@+id/textView"
        android:layout_width="464dp"
        android:layout_height="88dp"
        android:background="@color/black"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="-10dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="96dp"
        android:layout_height="93dp"
        android:backgroundTint="#000000"
        app:srcCompat="@mipmap/back"
        tools:layout_editor_absoluteX="-11dp"
        tools:layout_editor_absoluteY="-11dp" />

    <TextView
        android:id="@+id/user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JOIN US"
        android:textColor="#ffffff"
        android:textSize="35dp"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/userEmail"
        android:layout_weight="1"
        android:ems="10"
        android:hint="EMAIL"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="113dp" />

    <Button
        android:id="@+id/chkBtn"
        android:layout_width="104dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:backgroundTint="#000000"
        android:text="중복체크"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="317dp"
        tools:layout_editor_absoluteY="109dp" />

    <EditText
        android:id="@+id/userPass"
        android:layout_weight="1"
        android:ems="10"
        android:hint="PASSWORD"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="170dp" />

    <EditText
        android:id="@+id/userBirth"
        android:layout_weight="1"
        android:ems="10"
        android:hint="생년월일"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="231dp" />

    <EditText
        android:id="@+id/userMajor"
        android:layout_weight="1"
        android:ems="10"
        android:hint="학과"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="292dp" />

    <EditText
        android:id="@+id/userstunum"
        android:layout_weight="1"
        android:ems="10"
        android:hint="학번"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="353dp" />
    <EditText
        android:id="@+id/userLicense"
        android:layout_weight="1"
        android:ems="10"
        android:hint="학생증"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="414dp" />

    <Button
        android:id="@+id/fileLicense"
        android:layout_width="104dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:backgroundTint="#000000"
        android:text="파일첨부"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="317dp"
        tools:layout_editor_absoluteY="414dp" />

    <EditText
        android:id="@+id/driver"
        android:layout_width="363dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=" 운전자는 아래 사항도 입력해주세요"
        android:textAlignment="center"
        android:textColor="#8f4f4f"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="480dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="37dp"
        android:layout_height="48dp"
        app:srcCompat="@mipmap/warning"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="486dp" />

    <EditText
        android:id="@+id/carNum"
        android:layout_weight="1"
        android:ems="10"
        android:hint="차량 번호"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="540dp" />

    <EditText
        android:id="@+id/carType"
        android:layout_weight="1"
        android:ems="10"
        android:hint="차종"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="601dp" />

    <EditText
        android:id="@+id/carLicense"
        android:layout_weight="1"
        android:ems="10"
        android:hint="면허증"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="662dp" />
    <Button
        android:id="@+id/fileLicense1"
        android:layout_width="104dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:backgroundTint="#000000"
        android:text="파일첨부"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="317dp"
        tools:layout_editor_absoluteY="662dp" />

    <EditText
        android:id="@+id/carInsurance"
        android:layout_weight="1"
        android:ems="10"
        android:hint="차량 보험"
        android:inputType="textEmailAddress"
        android:layout_width="238dp"
        android:layout_height="55dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="723dp" />

    <Button
        android:id="@+id/fileLicense2"
        android:layout_width="104dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:backgroundTint="#000000"
        android:text="파일첨부"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="317dp"
        tools:layout_editor_absoluteY="723dp" />

    <CheckBox
        android:layout_width="250dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="780dp"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:text="모두 동의합니다." />

    <CheckBox
        android:layout_width="250dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="810dp"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:text="[필수] 개인정보 동의 약관" />

    <CheckBox
        android:layout_width="250dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="840dp"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:text="[필수] 서비스 이용 약관" />

    <CheckBox
        android:layout_width="250dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="870dp"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:text="[선택] 마케팅 이용 약관" />

    <Button
        android:id="@+id/registerBtn"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:backgroundTint="#000000"
        android:text="회원가입"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="948dp" />


</androidx.constraintlayout.widget.ConstraintLayout>